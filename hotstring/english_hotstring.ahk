{    
    StringGetPos, startPos, A_ThisHotkey, :, l2		; Left Second

    if !english {
        english := SubStr(A_ThisHotkey, startPos+2)        
    }

    if !del_eng {                    
        hotkey := SubStr(A_ThisHotkey, startPos+2)    
        del_eng := StrLen(hotkey)
    }

    korean := isKorean()
